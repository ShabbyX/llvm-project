    "//${llvm_root}clang/lib/Serialization",
    "//${llvm_root}llvm/lib/ExecutionEngine/Orc",
    "//${llvm_root}llvm/lib/IR",
    "//${llvm_root}llvm/lib/Option",
    "//${llvm_root}llvm/lib/Support",
    "//${llvm_root}llvm/lib/Target",
    "//${llvm_root}llvm/lib/Target:NativeTarget",
