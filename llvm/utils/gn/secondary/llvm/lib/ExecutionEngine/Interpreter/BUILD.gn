import("//build_overrides/llvm.gni")

static_library("Interpreter") {
  output_name = "LLVMInterpreter"
  deps = [
    "//${llvm_root}llvm/include/llvm/Config:config",
    "//${llvm_root}llvm/lib/CodeGen",
    "//${llvm_root}llvm/lib/ExecutionEngine",
    "//${llvm_root}llvm/lib/IR",
    "//${llvm_root}llvm/lib/Support",
  ]
  sources = [
    "Execution.cpp",
    "ExternalFunctions.cpp",
    "Interpreter.cpp",
  ]
  configs += llvm_code_override_configs_append
  configs -= llvm_code_override_configs_remove
}
