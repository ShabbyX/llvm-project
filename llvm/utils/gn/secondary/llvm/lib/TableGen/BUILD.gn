import("//build_overrides/llvm.gni")

static_library("TableGen") {
  output_name = "LLVMTableGen"
  deps = [ "//${llvm_build_root}/secondary/llvm/lib/Support" ]
  sources = [
    "DetailedRecordsBackend.cpp",
    "Error.cpp",
    "JSONBackend.cpp",
    "Main.cpp",
    "Parser.cpp",
    "Record.cpp",
    "SetTheory.cpp",
    "StringMatcher.cpp",
    "TGLexer.cpp",
    "TGParser.cpp",
    "TableGenBackend.cpp",
    "TableGenBackendSkeleton.cpp",
  ]
  configs += llvm_code_override_configs_append
  configs -= llvm_code_override_configs_remove
}
